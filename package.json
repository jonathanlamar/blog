  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint-check": "eslint --ignore-path .eslintignore --ext .ts,.tsx .",
    "lint-fix": "eslint --fix --ignore-path .eslintignore --ext .ts,.tsx .",
    "format-check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx|json)\"",
    "format-fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx|json)\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm run lint-check && npm run format-check",
    "typecheck": "tsc --noEmit"
